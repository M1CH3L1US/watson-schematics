{
  "name": "@watsonjs/schematics",
  "version": "0.0.1",
  "description": "Schematics for the Watson framework",
  "main": "index.js",
  "scripts": {
    "build": "rimraf -rf dist && tsc --project tsconfig.lib.json",
    "clean": "gulp clean:src",
    "postbuild": "npm run copy:collection && npm run copy:lib",
    "copy:collection": "cpx src/collection.json dist && cpx 'src/lib/**/schema.json' dist/lib",
    "copy:lib": "cpx 'src/lib/**/files/**/*.*' dist/lib"
  },
  "authors": [
    "Micheal S."
  ],
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@angular-devkit/core": "^11.1.2",
    "@angular-devkit/schematics": "^11.1.2",
    "@angular-devkit/schematics-cli": "^0.1101.2",
    "@types/node": "^14.14.22",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "typescript": "^3.4.5 || ^4.0.0"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "rimraf": "^3.0.2",
    "ts-node": "9.1.1"
  }
}
